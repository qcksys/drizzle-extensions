{
  "name": "@qcksys/drizzle-extensions",
  "version": "0.0.5",
  "type": "module",
  "exports": {
    "./mysql": {
      "types": "./dist/mysql.d.ts",
      "import": "./dist/mysql.js",
      "require": "./dist/mysql.cjs"
    },
    "./pgsql": {
      "types": "./dist/pgsql.d.ts",
      "import": "./dist/pgsql.js",
      "require": "./dist/pgsql.cjs"
    },
    "./sqlite": {
      "types": "./dist/sqlite.d.ts",
      "import": "./dist/sqlite.js",
      "require": "./dist/sqlite.cjs"
    },
    "./sqlite-expo": {
      "types": "./dist/sqlite-expo.d.ts",
      "import": "./dist/sqlite-expo.js",
      "require": "./dist/sqlite-expo.cjs"
    }
  },
  "scripts": {
    "test": "bun test",
    "build": "bun run build.ts",
    "prepublishOnly": "bun run build",
    "biome:check:unsafe": "biome check --write --unsafe",
    "biome:ci": "biome ci"
  },
  "files": ["dist"],
  "keywords": ["drizzle-orm"],
  "license": "MIT",
  "homepage": "https://github.com/qcksys/drizzle-extensions#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qcksys/drizzle-extensions.git"
  },
  "bugs": "https://github.com/qcksys/drizzle-extensions/issues",
  "author": "Tom Alle <tom@qcksys.com>",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "^1.2.14",
    "@types/react": "^19.1.6",
    "bun-plugin-dts": "^0.3.0",
    "drizzle-orm": "^0.43.1",
    "expo-sqlite": "^15.2.10",
    "react": "^19.1.0",
    "typescript": "^5.8.3"
  },
  "trustedDependencies": ["@biomejs/biome"]
}

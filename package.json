{
	"name": "@qcksys/drizzle-extensions",
	"version": "0.2.0",
	"type": "module",
	"exports": {
		"./onConflictDoUpdate": {
			"import": "./dist/onConflictDoUpdate.js",
			"require": "./dist/onConflictDoUpdate.cjs"
		},
		"./onDuplicateKeyUpdate": {
			"import": "./dist/onDuplicateKeyUpdate.js",
			"require": "./dist/onDuplicateKeyUpdate.cjs"
		},
		"./useLiveTablesQuery": {
			"import": "./dist/useLiveTablesQuery.js",
			"require": "./dist/useLiveTablesQuery.cjs"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"test": "vitest run",
		"test:integration": "vitest run --config vitest.integration.config.ts",
		"test:all": "vitest run && vitest run --config vitest.integration.config.ts",
		"build": "tsdown",
		"prepublishOnly": "pnpm run build",
		"biome:check:unsafe": "biome check --write --unsafe",
		"biome:ci": "biome ci",
		"prepare": "husky",
		"precommit": "pnpm run biome:check:unsafe && tsc && pnpm run test"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"drizzle-orm",
		"qcksys",
		"expo",
		"react-native",
		"sqlite"
	],
	"license": "MIT",
	"homepage": "https://github.com/qcksys/drizzle-extensions#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/qcksys/drizzle-extensions.git"
	},
	"bugs": "https://github.com/qcksys/drizzle-extensions/issues",
	"author": "Tom Alle <tom@qcksys.com>",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@testcontainers/mysql": "^10.25.0",
		"@testcontainers/postgresql": "^10.25.0",
		"@types/react": "^19.2.7",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.7",
		"expo-sqlite": "^15.2.14",
		"husky": "^9.1.7",
		"mysql2": "^3.14.1",
		"postgres": "^3.4.7",
		"react": "^19.2.0",
		"testcontainers": "^10.25.0",
		"tsdown": "^0.16.8",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.14"
	}
}
